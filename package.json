{
    "name": "hubgine",
    "version": "0.0.3",
    "private": true,
    "description": "Hubgine isomorphic app",
    "scripts": {
        "build": "npm-run-all build:*",
        "build:css": "node-sass public/css/main.scss > public/css/main.css",
        "build:js": "browserify app/main.js -t [ babelify --presets [es2015 react] --plugins [transform-object-rest-spread recharts transform-class-properties] ] -o public/js/bundle.js",
        "buildproduction": "npm-run-all buildproduction:*",
        "buildproduction:css": "node-sass public/css/main.scss | cleancss -O2 -o public/css/main.css",
        "buildproduction:js": "NODE_ENV=production browserify app/main.js -t [ loose-envify --NODE_ENV production ] -t [ babelify --presets [es2015 react] --plugins [transform-object-rest-spread recharts transform-react-remove-prop-types transform-class-properties] ] | uglifyjs -m -c -o public/js/bundle.js",
        "postinstall": "npm run buildproduction",
        "start": "node server.js",
        "start:production": "NODE_ENV=production pm2 start server.js",
        "watch": "npm-run-all --parallel watch:*",
        "watch:css": "nodemon -e scss -w public/css -x npm run build:css",
        "watch:js": "watchify app/main.js -t [ babelify --presets [es2015 react] --plugins [transform-object-rest-spread recharts transform-class-properties] ] -v -o public/js/bundle.js"
    },
    "dependencies": {
        "babel-core": "^6.26.0",
        "babel-plugin-recharts": "^1.1.1",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babelify": "^8.0.0",
        "body-parser": "^1.18.2",
        "browserify": "^15.2.0",
        "clean-css-cli": "^4.1.10",
        "compression": "^1.7.1",
        "express": "^4.16.2",
        "lodash": "^4.17.5",
        "loose-envify": "^1.3.1",
        "mime": "^2.2.0",
        "moment": "^2.20.1",
        "morgan": "^1.9.0",
        "newrelic": "^2.7.0",
        "node-sass": "^4.7.2",
        "nodemon": "^1.14.12",
        "npm-run-all": "^4.1.2",
        "pm2": "^2.9.3",
        "prop-types": "^15.6.0",
        "pug": "^2.0.0-rc.4",
        "react": "16.2.0",
        "react-datetime": "^2.12.0",
        "react-dom": "16.2.0",
        "react-recaptcha": "^2.3.6",
        "react-redux": "5.0.6",
        "react-router-dom": "^4.2.2",
        "recharts": "^1.0.0-beta.10",
        "redux": "^3.7.2",
        "redux-cookie": "^0.5.9",
        "redux-logger": "^3.0.6",
        "redux-promise": "^0.5.3",
        "redux-thunk": "^2.2.0",
        "serve-favicon": "^2.4.5",
        "uglify-js": "^3.3.9",
        "universal-cookie-express": "^2.1.2",
        "uuid": "^3.2.1",
        "validator": "git+ssh://git@bitbucket.org/TWEET-BOT/validator.git#V1.2.4",
        "watchify": "^3.10.0"
    }
}
