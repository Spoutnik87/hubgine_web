{
  "name": "hubgine",
  "version": "0.0.2",
  "description": "Hubgine isomorphic app",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:css": "node-sass public/css/main.scss > public/css/main.css",
    "build:js": "browserify app/main.js -t [ babelify --presets [es2015 react] --plugins transform-object-rest-spread recharts ] -o public/js/bundle.js",
    "buildproduction": "npm-run-all buildproduction:*",
    "buildproduction:css": "node-sass public/css/main.scss | cleancss -O2 -o public/css/main.css",
    "buildproduction:js": "NODE_ENV=production browserify app/main.js -t [ loose-envify --NODE_ENV production ] -t [ babelify --presets [es2015 react] --plugins transform-object-rest-spread recharts transform-react-remove-prop-types ] | uglifyjs -m -c -o public/js/bundle.js",
    "postinstall": "npm run build",
    "start": "node server.js",
    "start:production": "pm2 start server.js -i 4",
    "watch": "npm-run-all --parallel watch:*",
    "watch:css": "nodemon -e scss -w public/css -x npm run build:css",
    "watch:js": "watchify app/main.js -t [ babelify --presets [es2015 react] --plugins transform-object-rest-spread recharts ] -v -o public/js/bundle.js"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-recharts": "^1.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "body-parser": "^1.17.1",
    "browserify": "^14.3.0",
    "clean-css-cli": "^4.0.12",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "express-validator": "^3.2.0",
    "loose-envify": "^1.3.1",
    "mime": "^1.3.4",
    "morgan": "^1.8.1",
    "newrelic": "^1.39.0",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "pm2": "^2.4.6",
    "prop-types": "^15.5.8",
    "pug": "^2.0.0-rc.1",
    "react": "15.5.4",
    "react-cookie": "^2.0.6",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "react-router-dom": "^4.1.1",
    "recharts": "^0.22.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.4.2",
    "uglify-js": "^2.8.23",
    "universal-cookie-express": "^2.0.5",
    "validator": "^7.0.0",
    "watchify": "^3.9.0"
  },
  "devDependencies": {},
  "engines": {
    "node": "7.10.0"
  }
}
