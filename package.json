{
  "name": "hubgine",
  "version": "0.0.2",
  "description": "Hubgine isomorphic app",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:css": "node-sass public/css/main.scss > public/css/main.css",
    "build:js": "browserify app/main.js -t [ babelify --presets [es2015 react] --plugins transform-object-rest-spread ] -o public/js/bundle.js",
    "postinstall": "npm run build",
    "start": "node server.js",
    "start:production": "pm2 start server.js -i 4",
    "watch": "npm-run-all --parallel watch:*",
    "watch:css": "nodemon -e scss -w public/css -x npm run build:css",
    "watch:js": "watchify app/main.js -t [ babelify --presets [es2015 react] --plugins transform-object-rest-spread ] -v -o public/js/bundle.js"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "body-parser": "^1.17.1",
    "browserify": "^14.3.0",
    "chart.js": "^2.5.0",
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-validator": "^3.2.0",
    "google-closure-compiler": "^20170409.0.0",
    "mime": "^1.3.4",
    "morgan": "^1.8.1",
    "newrelic": "^1.38.2",
    "node-sass": "^4.5.2",
    "nodemailer": "^4.0.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "pm2": "^2.4.5",
    "prop-types": "^15.5.8",
    "pug": "^2.0.0-beta.12",
    "react": "15.5.4",
    "react-chartjs-2": "^2.0.5",
    "react-cookie": "^2.0.6",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "react-router": "^3.0.5",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.4.2",
    "socket.io": "^1.7.3",
    "universal-cookie-express": "^2.0.4",
    "validator": "^7.0.0",
    "watchify": "^3.9.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {},
  "engines": {
    "node": "7.9.0"
  }
}
