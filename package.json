{
  "name": "hubgine",
  "version": "0.0.2",
  "private": true,
  "description": "Hubgine isomorphic app",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:css": "node-sass public/css/main.scss > public/css/main.css",
    "build:js": "browserify app/main.js -t [ babelify --presets [es2015 react] --plugins [transform-object-rest-spread recharts transform-class-properties] ] -o public/js/bundle.js",
    "buildproduction": "npm-run-all buildproduction:*",
    "buildproduction:css": "node-sass public/css/main.scss | cleancss -O2 -o public/css/main.css",
    "buildproduction:js": "NODE_ENV=production browserify app/main.js -t [ loose-envify --NODE_ENV production ] -t [ babelify --presets [es2015 react] --plugins [transform-object-rest-spread recharts transform-react-remove-prop-types transform-class-properties] ] | uglifyjs -m -c -o public/js/bundle.js",
    "postinstall": "npm run buildproduction",
    "start": "node server.js",
    "start:production": "pm2 start server.js -i 4",
    "watch": "npm-run-all --parallel watch:*",
    "watch:css": "nodemon -e scss -w public/css -x npm run build:css",
    "watch:js": "watchify app/main.js -t [ babelify --presets [es2015 react] --plugins [transform-object-rest-spread recharts transform-class-properties] ] -v -o public/js/bundle.js"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-recharts": "^1.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "body-parser": "^1.17.2",
    "browserify": "^14.4.0",
    "clean-css-cli": "^4.1.9",
    "compression": "^1.7.0",
    "express": "^4.15.4",
    "lodash": "^4.17.4",
    "loose-envify": "^1.3.1",
    "mime": "^1.4.0",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "newrelic": "^2.2.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.0",
    "npm-run-all": "^4.1.1",
    "pm2": "^2.6.1",
    "prop-types": "^15.5.10",
    "pug": "^2.0.0-rc.3",
    "react": "15.6.1",
    "react-cookie": "^2.1.0",
    "react-datetime": "^2.10.1",
    "react-dom": "15.6.1",
    "react-recaptcha": "^2.3.5",
    "react-redux": "5.0.6",
    "react-router-dom": "^4.2.2",
    "recharts": "^1.0.0-alpha.4",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.4.3",
    "uglify-js": "^3.0.28",
    "universal-cookie-express": "^2.1.0",
    "uuid": "^3.1.0",
    "validator": "git+ssh://git@bitbucket.org/TWEET-BOT/validator.git#V1.1.4",
    "watchify": "^3.9.0"
  },
  "devDependencies": {},
  "engines": {
    "node": "8.4.0"
  }
}
