{
  "name": "hubgine",
  "version": "0.0.4",
  "private": true,
  "description": "Hubgine isomorphic app",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:css": "node-sass public/css/main.scss > public/css/main.css",
    "build:js": "browserify app/main.js -t [ babelify --presets [es2015 react] --plugins [transform-object-rest-spread recharts transform-class-properties] ] -o public/js/bundle.js",
    "buildproduction": "npm-run-all buildproduction:*",
    "buildproduction:css": "node-sass public/css/main.scss | cleancss -O2 -o public/css/main.css",
    "buildproduction:js": "NODE_ENV=production browserify app/main.js -t [ loose-envify --NODE_ENV production ] -t [ babelify --presets [es2015 react] --plugins [transform-object-rest-spread recharts transform-react-remove-prop-types transform-class-properties] ] | uglifyjs -m -c -o public/js/bundle.js",
    "postinstall": "npm run buildproduction",
    "start": "node server.js",
    "start:production": "pm2 start server.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:css": "nodemon -e scss -w public/css -x npm run build:css",
    "watch:js": "watchify app/main.js -t [ babelify --presets [es2015 react] --plugins [transform-object-rest-spread recharts transform-class-properties] ] -v -o public/js/bundle.js",
    "test": "mocha"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-recharts": "^1.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "body-parser": "^1.18.3",
    "browserify": "^16.2.3",
    "clean-css-cli": "^4.2.1",
    "compression": "^1.7.4",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "loose-envify": "^1.4.0",
    "mime": "^2.4.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "newrelic": "^5.6.2",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "pm2": "^3.4.1",
    "prop-types": "^15.7.2",
    "pug": "^2.0.3",
    "react": "16.8.5",
    "react-datetime": "^2.16.3",
    "react-dom": "16.8.5",
    "react-recaptcha": "^2.3.10",
    "react-redux": "6.0.1",
    "react-router-dom": "^5.0.0",
    "recharts": "^1.5.0",
    "redux": "^4.0.1",
    "redux-cookie": "^0.5.9",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "serve-favicon": "^2.5.0",
    "uglify-js": "^3.5.2",
    "universal-cookie-express": "^3.1.0",
    "uuid": "^3.3.2",
    "validator": "git+ssh://git@github.com:Spoutnik87/hubgine_validator.git#V1.2.7",
    "watchify": "^3.11.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.0.2"
  }
}
